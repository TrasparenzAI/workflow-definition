      "taskReferenceName": "refresh_rule_config_task_ref",
      "inputParameters": {
        "http_request": {
          "uri": "${workflow.input.rule_base_url}/actuator/refresh",
          "method": "POST",
          "connectionTimeOut": 30000,
          "readTimeOut": 30000,
          "accept": "application/json",
          "contentType": "application/json"
        }
      },
      "type": "HTTP_WEBCLIENT"
    },
