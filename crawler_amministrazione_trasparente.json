          "startDelay": 0,
          "joinOn": [],
          "optional": false,
          "defaultExclusiveJoinTask": [],
          "asyncComplete": false,
          "loopOver": [],
          "onStateChange": {}
        },
        {
          "name": "Set Last Page",
          "taskReferenceName": "set_last_page_ref",
          "inputParameters": {
            "last_page": "${companies_task_ref.output.response.body.last}"
          },
          "type": "SET_VARIABLE"
        }        
      ],
      "evaluatorType": "value-param",
      "onStateChange": {}
    }
  ],
  "inputParameters": [
    "codice_categoria",
