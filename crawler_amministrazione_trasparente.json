        {
          "name": "Current time start",
          "taskReferenceName": "get_current_time_start_ref",
          "type": "INLINE",
          "inputParameters": {
            "evaluatorType": "javascript",
            "expression": "function e() { return Date.now(); } e();"
          }
        },
        {
            "name": "Result JSON Add workflow",
            "taskReferenceName": "result_json_add_workflow_ref",
            "inputParameters": {
              "workflowId": "${workflow.workflowId}",
              "codiceIpa": "${workflow.input.codice_ipa}",
              "rootRule": "${workflow.input.root_rule}",
              "status": "${workflow.status}",
              "createTime": "${get_current_time_start_ref.output.result}",              
              "startTime": "${get_current_time_start_ref.output.result}",              
              "queryExpression": "{workflowId: .workflowId, codiceIpa: .codiceIpa, rootRule: .rootRule, status: .status, startTime: .startTime, createTime: .createTime}"
            },
            "type": "JSON_JQ_TRANSFORM",
            "retryCount": 0,
            "startDelay": 0,
            "optional": true,
            "asyncComplete": false
        },
        {
            "name": "Result Add Workflow",
            "taskReferenceName": "result_add_workflow_ref",
            "inputParameters": {
              "http_request": {
                "uri": "${workflow.input.result_base_url}/v1/workflows",
                "body": "${result_json_add_workflow_ref.output.result}",
                "method": "PUT",
                "connectionTimeOut": 120000,
                "readTimeOut": 120000
              }
            },
            "type": "HTTP_WEBCLIENT",
            "retryCount": 0,
            "optional": true,
            "asyncComplete": false
        },
        {
            "name": "Refresh configuration of rules",
            "taskReferenceName": "refresh_rule_config_task_ref",
            "inputParameters": {
                "http_request": {
                    "uri": "${workflow.input.rule_base_url}/actuator/refresh",
                    "method": "POST",
                    "connectionTimeOut": 30000,
                    "readTimeOut": 30000,
                    "accept": "application/json",
                    "contentType": "application/json"
                }
            },
            "type": "HTTP_WEBCLIENT"
        },
        {
            "name": "Set Initial Last Page",
            "taskReferenceName": "set_initial_last_page_ref",
            "inputParameters": {
                "last_page": false
            },
            "type": "SET_VARIABLE"
        },
        {
            "name": "Parent Workflow ID",
            "taskReferenceName": "parent_workflow_id_ref",
            "inputParameters": {
                "expression": "(function(){if ($.s && $.s.length() > 0) { return $.s; } else { return  $.l; }})();",
                "evaluatorType": "javascript",
                "s": "${workflow.input.parent_workflow_id}",
                "l": "${workflow.workflowId}"
            },
            "type": "INLINE"
        },
        {
            "name": "Page divide size",
            "taskReferenceName": "page_divide_size_ref",
            "inputParameters": {
                "expression": "(function(){ return Math.floor($.s / 10) })();",
                "evaluatorType": "javascript",
                "s": "${workflow.input.page_size}"
            },
            "type": "INLINE"
        },
        {
            "name": "Pagination task",
            "taskReferenceName": "pagination_task_ref",
            "inputParameters": {
                "last_page": "${workflow.variables.last_page}"
            },
            "type": "DO_WHILE",
            "loopCondition": "if ($.last_page) { false; } else { true; }",
            "loopOver": [
                {
                    "name": "iteration_task",
                    "taskReferenceName": "iteration_task_ref",
                    "inputParameters": {
                        "expression": "(function(){ return ($.i - 1 | 0);})();",
                        "evaluatorType": "javascript",
                        "i": "${pagination_task_ref.output.iteration}"
                    },
