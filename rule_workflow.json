                          "body": "${crawler_html_source_ref.output.response.body.htmlPage}",
                          "method": "POST",
                          "connectionTimeOut": 1000,
                          "readTimeOut": 1000
                        }
                      },
                      "type": "HTTP",
                      "startDelay": 0,
                      "optional": true,
                      "asyncComplete": false
                    },
                    {
                      "name": "Switch Rule HTML Source",
                      "taskReferenceName": "switch_rule_html_source_ref",
                      "inputParameters": {
                        "switchCaseValue": "${rule_html_source_ref.status}"
                      },
                      "type": "SWITCH",
                      "decisionCases": {
                        "COMPLETED_WITH_ERRORS": [
                          {
                            "name": "Result Rule HTTP Source Failed",
                            "taskReferenceName": "result_rule_http_source_failed_ref",
                            "inputParameters": {
                              "http_request": {
                                "uri": "https://dica33.ba.cnr.it/result-service/v1/results",
                                "body": "{\"company\": {\"idIpa\": ${ipa_ref.output.result.id}, \"codiceIpa\": \"${ipa_ref.output.result.codiceIpa}\", \"denominazioneEnte\": ${denominazione_ente_trasform_ref.output.result}, \"codiceFiscaleEnte\": \"${ipa_ref.output.result.codiceFiscaleEnte}\", \"tipologia\": \"${ipa_ref.output.result.tipologia}\", \"codiceCategoria\": \"${ipa_ref.output.result.codiceCategoria}\", \"codiceNatura\": \"${ipa_ref.output.result.codiceNatura}\", \"acronimo\": \"${ipa_ref.output.result.acronimo}\", \"sitoIstituzionale\": \"${ipa_ref.output.result.sitoIstituzionale}\", \"sorgente\": \"${ipa_ref.output.result.sorgente}\"}, \"realUrl\": \"${crawler_http_stream_ref.output.response.body.realUrl}\", \"storageData\": {\"objectBucket\": \"${crawler_http_stream_ref.output.response.body.objectBucket}\", \"objectId\": \"${crawler_http_stream_ref.output.response.body.objectId}\", \"objectResult\": \"${crawler_http_stream_ref.output.response.body.objectResult}\", \"screenshotBucket\": \"${crawler_http_stream_ref.output.response.body.screenshotBucket}\", \"screenshotId\": \"${crawler_http_stream_ref.output.response.body.screenshotId}\", \"screenshotResult\": \"${crawler_http_stream_ref.output.response.body.screenshotResult}\"}, \"status\": 400, \"workflowId\": \"${workflow.input.parent_workflow_id}\"}",
                                "method": "PUT",
                                "connectionTimeOut": 10000,
                                "readTimeOut": 30000
                              }
                            },
                            "type": "HTTP",
                            "startDelay": 0,
                            "optional": false,
                            "asyncComplete": false
                          }
                        ],
                        "COMPLETED": [
                          {
                            "name": "Result Rule HTTP Source Completed",
                            "taskReferenceName": "result_rule_http_source_completed_ref",
                            "inputParameters": {
                              "http_request": {
                                "uri": "https://dica33.ba.cnr.it/result-service/v1/results",
                                "body": "{\"company\": {\"idIpa\": ${ipa_ref.output.result.id}, \"codiceIpa\": \"${ipa_ref.output.result.codiceIpa}\", \"denominazioneEnte\": ${denominazione_ente_trasform_ref.output.result}, \"codiceFiscaleEnte\": \"${ipa_ref.output.result.codiceFiscaleEnte}\", \"tipologia\": \"${ipa_ref.output.result.tipologia}\", \"codiceCategoria\": \"${ipa_ref.output.result.codiceCategoria}\", \"codiceNatura\": \"${ipa_ref.output.result.codiceNatura}\", \"acronimo\": \"${ipa_ref.output.result.acronimo}\", \"sitoIstituzionale\": \"${ipa_ref.output.result.sitoIstituzionale}\", \"sorgente\": \"${ipa_ref.output.result.sorgente}\"}, \"realUrl\": \"${crawler_http_stream.output.response.body.realUrl}\", \"storageData\": {\"objectBucket\": \"${crawler_http_stream.output.response.body.objectBucket}\", \"objectId\": \"${crawler_http_stream.output.response.body.objectId}\", \"objectResult\": \"${crawler_http_stream.output.response.body.objectResult}\", \"screenshotBucket\": \"${crawler_http_stream.output.response.body.screenshotBucket}\", \"screenshotId\": \"${crawler_http_stream.output.response.body.screenshotId}\", \"screenshotResult\": \"${crawler_http_stream.output.response.body.screenshotResult}\"}, \"url\": \"${rule_html_source_ref.output.response.body.url}\", \"ruleName\": \"${rule_html_source_ref.output.response.body.ruleName}\", \"term\": \"${rule_html_source_ref.output.response.body.term}\", \"content\": \"${rule_html_source_ref.output.response.body.content}\", \"status\": ${rule_html_source_ref.output.response.body.status}, \"score\": ${rule_html_source_ref.output.response.body.score}, \"workflowId\": \"${workflow.input.parent_workflow_id}\", \"leaf\": ${rule_html_source_ref.output.response.body.isLeaf}}",
                                "method": "PUT",
                                "connectionTimeOut": 10000,
                                "readTimeOut": 30000
                              }
                            },
                            "type": "HTTP",
                            "startDelay": 0,
                            "optional": false,
                            "asyncComplete": false
                          }
                        ]
                      },
                      "startDelay": 0,
                      "optional": false,
                      "asyncComplete": false,
                      "evaluatorType": "value-param",
                      "expression": "switchCaseValue"
                    }
                  ],
                  "COMPLETED": [
                    {
                      "name": "Content trasform Task",
                      "taskReferenceName": "content_trasform_ref",
                      "inputParameters": {
                        "content": "${rule_http_stream_ref.output.response.body.content}",
                        "expression": "JSON.stringify($.content)",
                        "evaluatorType": "javascript"
                      },
                      "type": "INLINE",
                      "startDelay": 0,
                      "optional": false,
                      "asyncComplete": false
                    },
                    {
                      "name": "Result Rule HTTP Stream Completed",
                      "taskReferenceName": "result_rule_http_stream_completed_ref",
                      "inputParameters": {
                        "http_request": {
                          "uri": "https://dica33.ba.cnr.it/result-service/v1/results",
                          "body": "{\"company\": {\"idIpa\": ${ipa_ref.output.result.id}, \"codiceIpa\": \"${ipa_ref.output.result.codiceIpa}\", \"denominazioneEnte\": ${denominazione_ente_trasform_ref.output.result}, \"codiceFiscaleEnte\": \"${ipa_ref.output.result.codiceFiscaleEnte}\", \"tipologia\": \"${ipa_ref.output.result.tipologia}\", \"codiceCategoria\": \"${ipa_ref.output.result.codiceCategoria}\", \"codiceNatura\": \"${ipa_ref.output.result.codiceNatura}\", \"acronimo\": \"${ipa_ref.output.result.acronimo}\", \"sitoIstituzionale\": \"${ipa_ref.output.result.sitoIstituzionale}\", \"sorgente\": \"${ipa_ref.output.result.sorgente}\"}, \"realUrl\": \"${crawler_http_stream_ref.output.response.body.realUrl}\", \"storageData\": {\"objectBucket\": \"${crawler_http_stream_ref.output.response.body.objectBucket}\", \"objectId\": \"${crawler_http_stream_ref.output.response.body.objectId}\", \"objectResult\": \"${crawler_http_stream_ref.output.response.body.objectResult}\", \"screenshotBucket\": \"${crawler_http_stream_ref.output.response.body.screenshotBucket}\", \"screenshotId\": \"${crawler_http_stream_ref.output.response.body.screenshotId}\", \"screenshotResult\": \"${crawler_http_stream_ref.output.response.body.screenshotResult}\"}, \"url\": \"${rule_http_stream_ref.output.response.body.url}\", \"ruleName\": \"${rule_http_stream_ref.output.response.body.ruleName}\", \"term\": \"${rule_http_stream_ref.output.response.body.term}\", \"content\": ${content_trasform_ref.output.result}, \"status\": ${rule_http_stream_ref.output.response.body.status}, \"score\": ${rule_http_stream_ref.output.response.body.score}, \"workflowId\": \"${workflow.input.parent_workflow_id}\", \"leaf\": ${rule_http_stream_ref.output.response.body.isLeaf}}",
                          "method": "PUT",
                          "connectionTimeOut": 10000,
                          "readTimeOut": 30000
                        }
                      },
                      "type": "HTTP",
                      "startDelay": 0,
                      "optional": false,
                      "asyncComplete": false
                    }
                  ]
                },
                "startDelay": 0,
                "optional": false,
                "asyncComplete": false,
                "evaluatorType": "value-param",
                "expression": "switchCaseValue"
              }
            ]
          },
          "defaultCase": [],
          "startDelay": 0,
          "optional": false,
          "asyncComplete": false,
          "evaluatorType": "value-param",
          "expression": "switchCaseValue"
        }
      ]
    }
  ],
  "inputParameters": [
    "companies",
    "crawler_save_object",
    "crawler_save_screenshot",
    "parent_workflow_id"
  ],
  "outputParameters": {},
  "schemaVersion": 2,
  "restartable": true,
  "workflowStatusListenerEnabled": false,
  "ownerEmail": "marco.spasiano@gmail.com",
